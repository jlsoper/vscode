{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dotnet run",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net5.0/MyApp.dll",
      "args": [
        //  "one",
        //  "two"
      ],
      "cwd": "${workspaceFolder}",

      // Debug Console (internalConsole) does not support inputs. [use integratedTerminal or externalTerminal]
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": ".NET attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
